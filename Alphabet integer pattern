input:
b 6
c 4

output:
b6 b7 b8 b9 b10 c1 c2 c3 c4 c5 c6

code:
#include<stdio.h>
#include <stdlib.h>

int main()
{
    char a,b;
    int a1,b1;
    scanf("%c %d\n%c %d",&a,&a1,&b,&b1);
    int c=a1,d;
    while(a<=b)
    {
        d=(a<b)?10:b1;
        while(c<=d)
        {
            printf("%c%d ",a,c);
            c++;
        }
        a++;
        c=1;
    }


}
